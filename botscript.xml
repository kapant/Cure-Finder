<context>
    <input pattern = "/start">
        <output value = "Привет!"/>
        <context>
            <input pattern = "repeat($Text,)">
                <var name = "Symptoms">
                    {
                        "symps": [
                            join("$Text", ',')
                        ]
                    }
                </var>
            </input>
        </context>
    </input>
</context>