<context>
    <input pattern = "/start">
        <output value = "Привет!"/>
        <context>
            <input pattern = "repeat($Number)">
                <var name = "Symptomes">
                    <script>
                        <![CDATA[
                            i = 0;
                            str = "[";
                            while ($Number[i]) {
                                str += '"' + $Number[i] + '",';
                                i++;
                            }
                            str - ',' + ']';
                            JSON.parse(str);
                        ]]>
                    </script>
                </var>
            </input>
        </context>
    </input>
</context>