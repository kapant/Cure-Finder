<context>
    <input pattern = "/start">
        <output value = "Привет!"/>
        <context>
            <pattern name = "List" value = "[у|меня] $Text(,| и|.)"/>
            <input pattern = "* [меня] repeat($List) *">
                <var name = "Symptomes">
                    <script>
                        <![CDATA[
                            i = 0;
                            str = "[";
                            while ($List[i]) {
                                if (i != 0) {
                                    str += ',';
                                }
                                str += '"' + $List[i] + '"';
                                i++;
                            }
                            str += ']';
                            JSON.parse(str);
                        ]]>
                    </script>
                </var>
                <output value = "У вас join($List, ", ")"/>
            </input>
        </context>
    </input>
</context>