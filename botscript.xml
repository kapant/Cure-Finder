<context>
    <input pattern = "/start">
        <output value = "Здравствуйте, что вас беспокоит?"/>
        <context id = "begin">
            <pattern name = "List" value = "[у|меня] $Text(,| и|.)"/>
            <input pattern = "* [меня] repeat($List) *">
                <var name = "Symptomes">
                    <script>
                        <![CDATA[
                            if ($List[1]) {
                                i = 0;
                                str = "[";
                                while ($List[i]) {
                                    if (i != 0) {
                                        str += ',';
                                    }
                                    str += '"' + $List[i] + '"';
                                    i++;
                                }
                                str += ']';
                                JSON.parse(str);
                            } else {
                                $List[0];
                            }
                        ]]>
                    </script>
                </var>
            </input>

            <input pattern = "* (ничего|хорош*|нормальн*) *">
                <output value = "Если будете чувствовать себя нехорошо - пишите."/>
                <context id = "begin"/>
            </input>

            <input pattern = "*">
                <output value = "Простите, я вас не понимаю. Что вас беспокоит?"/>
                <context id = "begin"/>
            </input>
        </context>
    </input>
</context>