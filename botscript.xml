<context>
    <input pattern = "/start">
        <output value = "Привет!"/>
        <context>
            <input pattern = "repeat($Text)">
                <var name = "Symptomes">
                    <script>
                        <![CDATA[
                            i = 0;
                            str = "[";
                            while ($Text[i]) {
                                if (i != 0) {
                                    str += ',';
                                }
                                str += '"' + $Text[i] + '"';
                                i++;
                            }
                            str += ']';
                            JSON.parse(str);
                        ]]>
                    </script>
                </var>
            </input>
        </context>
    </input>
</context>