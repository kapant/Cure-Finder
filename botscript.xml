<context>
    <input pattern = "/start">
        <output value = "Здравствуйте, что вас беспокоит?"/>
        <context id = "begin">
            <pattern name = "List" value = "[у[меня]] $Text(,| и|.)"/>
            <input pattern = "[у] [меня] [беспокоит] repeat($List)">
                <var name = "Symptomes">
                    <script>
                        <![CDATA[
                            if (typeof($List) == "string") {
                                $List;
                            } else {
                                i = 0;
                                str = "[";
                                while ($List[i]) {
                                    if (i != 0) {
                                        str += ',';
                                    }
                                    str += '"' + $List[i] + '"';
                                    i++;
                                }
                                str += ']';
                                JSON.parse(str);
                            }
                        ]]>
                    </script>
                </var>

                <context>
                    <var name = "Cures">
                        <script>
                            <![CDATA[
                                JSON.parse('
                                    [
                                        {
                                            "name": "cure1",
                                            "symp": "symp1"
                                        },
                                        {
                                            "name": "cure2",
                                            "symp": "symp2"
                                        },
                                        {
                                            "name": "cure3",
                                            "symp": "symp3"
                                        },
                                        {
                                            "name": "cure4",
                                            "symp": "symp4"
                                        }
                                    ]
                                ');
                            ]]>
                        </script>
                    </var>
                </context>
            </input>

            <input pattern = "* (ничего|хорош*|нормальн*) *">
                <output value = "Если будете чувствовать себя нехорошо - пишите."/>
                <context id = "begin"/>
            </input>

            <input pattern = "*">
                <output value = "Простите, я вас не понимаю. Что вас беспокоит?"/>
                <context id = "begin"/>
            </input>
        </context>
    </input>
</context>