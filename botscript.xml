<context>
    <input pattern = "/start">
        <output value = "Привет!"/>
        <context>
            <input pattern = "repeat($Number)">
                <var name = "Symptoms">
                    <script>
                        <![CDATA[
                            i = 0;
                            str = "";
                            while ($Number[i]) {
                                str += '"' + $Number[i] + '"';
                                i++;
                            }
                            str;
                        ]]>
                    </script>
                </var>
                <output value = "$Symptoms"/>
            </input>
        </context>
    </input>
</context>