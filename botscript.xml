<context>
    <input pattern = "/start">
        <output value = "Привет!"/>
        <context>
            <input pattern = "repeat($Number)">
                <var name = "Symptoms">
                    <![CDATA[
                        i = 0;
                        str = "";
                        while ($Number[i]) {
                            str += '"' + $Number[i] + '"';
                            i++;
                        }
                        str;
                    ]]>
                </var>
            </input>
        </context>
    </input>
</context>